{
  "name": "moldable-desktop",
  "private": true,
  "version": "0.1.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "icon": "tauri icon public/app-icon.png && rm -rf src-tauri/icons/ios src-tauri/icons/android",
    "clean": "rm -rf dist .turbo src-tauri/target",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:rust": "cargo test --manifest-path src-tauri/Cargo.toml",
    "test:all": "vitest run && cargo test --manifest-path src-tauri/Cargo.toml"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.29",
    "@moldable-ai/ai": "workspace:*",
    "@moldable-ai/ui": "workspace:*",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.5.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-process": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "@tauri-apps/plugin-updater": "^2",
    "ai": "^6.0.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.0",
    "fflate": "^0.8.2",
    "framer-motion": "^12.15.0",
    "lucide-react": "^0.554.0",
    "react": "^19",
    "react-dom": "^19",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@moldable-ai/eslint-config": "workspace:*",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@tauri-apps/cli": "^2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}

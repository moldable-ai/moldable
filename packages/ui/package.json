{
  "name": "@moldable-ai/ui",
  "version": "0.1.1",
  "description": "Shared UI components for Moldable applications",
  "author": "Desiderata LLC",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/moldable-ai/moldable.git",
    "directory": "packages/ui"
  },
  "homepage": "https://moldable.sh",
  "bugs": {
    "url": "https://github.com/moldable-ai/moldable/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./src/styles/index.css"
  },
  "files": [
    "dist",
    "src/styles",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.0",
    "framer-motion": "^12.15.0",
    "lucide-react": "^0.473.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.21.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4"
  },
  "devDependencies": {
    "@moldable-ai/eslint-config": "workspace:*",
    "@moldable-ai/prettier-config": "workspace:*",
    "@moldable-ai/typescript-config": "workspace:*",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
